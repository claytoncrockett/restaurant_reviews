<h1>New Review</h1>
<%= form_for @review do |f| %>
  <%= f.label :title, "Restaurant Name" %>
  <%= f.text_field :title %>

  <%= f.label :author %>
  <%= f.text_field :author %>

  <%= f.label :rating %>
  <%= f.text_field :rating %>

  <%= f.label :description %>
  <%= f.text_area :description %>

  <%= f.submit %>
<% end %>